{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/node(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "main.py",
      "maxDuration": 15
    }
  ]
}
